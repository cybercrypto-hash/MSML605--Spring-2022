        "    for line in lines:\n",
        "      line = line.replace('\"','').strip().split(\",\")\n",
        "      if line[0] not in allNames:\n",
        "        oppositeGender = \"boy\" if line[-1] == \"girl\" else \"girl\"\n",
        "        allNames.update({line[0]: {oppositeGender: {}, line[-1]: \n",
        "          {line[1]: float(line[2])}\n",
        "        }})\n",
        "      else:\n",
        "        allNames[line[0]][line[-1]][line[1]] = float(line[2])\n",
        "    namesPopularity = {\"boy\": {}, \"girl\": {}}\n",
        "    for year in allNames:\n",
        "      genders = (\"boy\", \"girl\")\n",
        "      for gender in genders:\n",
        "        names = allNames[year][gender]\n",
        "        for name, popularity in names.items():\n",
        "          if name not in namesPopularity[gender]:\n",
        "            namesPopularity[gender][name] = popularity\n",
        "          else:\n",
        "            namesPopularity[gender][name] += popularity\n",
        "    mostPopularBoyName = max(namesPopularity[\"boy\"], key=namesPopularity[\"boy\"].get)\n",
        "    mostPopularGirlName = max(namesPopularity[\"girl\"], key=namesPopularity[\"girl\"].get)\n",
        "    sharedNames = [k for k in namesPopularity[\"boy\"].keys() if k in namesPopularity[\"girl\"].keys()]\n",
        "    mostPopularName = max(sharedNames, key=lambda name: namesPopularity[\"boy\"][name] + namesPopularity[\"girl\"][name])\n",
        "    print(mostPopularName)\n",
        "    print(mostPopularGirlName)\n",
        "    print(mostPopularBoyName)\n",
        "\n",
        "\n",
        "\n",
        "  for year, genders in allNames.items():\n",
        "    most_popular_boy_name = max(genders['boy',], key=genders['boy'].get)\n",
